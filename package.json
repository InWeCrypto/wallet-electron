{
	"name": "inwemgr",
	"version": "1.0.0",
	"description": "dsds",
	"main": "main.js",
	"scripts": {
		"dev": "concurrently \"webpack  --config webpack.config.js --progress --profile&& electron .\" \"cross-env NODE_ENV=production webpack --watch\"",
		"compile": "webpack --config webpack.config.js --progress --profile",
		"postinstall": "electron-builder install-app-dep && webpack  --progress --profile --config webpack.dll.config.js",
		"pack": "electron-builder --dir",
		"dist": "webpack --config webpack.dll.config.js && electron-builder"
	},
	"build": {
		"appId": "com.electron.inwecrypto",
		"files": [
			"./main.js",
			"resources/index.html",
			"resources/"
		],
		"dmg": {
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			]
		},
		"win": {
			"target": "nsis",
			"icon": "build/icon.ico"
		}
	},
