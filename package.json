{
	"name": "InWeCrypto",
	"version": "1.0.24",
	"description": "InWeCrypto",
	"main": "main.js",
	"scripts": {
		"dev": "concurrently \"webpack  --config webpack.config.js --progress --profile&& electron .\" \"webpack --watch\"",
		"compile": "webpack --config webpack.dll.config.js --progress --profile",
		"watch": "webpack --watch",
		"postinstall": "webpack --config webpack.dll.config.js",
		"pack:mac": "webpack &&electron-builder --win",
		"pack:win": "webpack &&electron-builder --darwin",
		"pack": "rm -rf ./dist && webpack && electron-builder -mw",
		"pack:all": "webpack && electron-builder -mw"
	},
	"release": "build",
	"build": {
		"productName": "InWeCrypto",
		"appId": "com.raistone.inwecrypto",
		"copyright": "Â©InWeCrypto 2018",
		"files": [
			"./main.js",
			"resources/index.html",
			"resources/"
		],
		"mac": {
			"icon": "./favicon.icns"
		},
		"publish": [
			{
				"provider": "generic",
				"url": "https://inwecrypto-china.oss-cn-shanghai.aliyuncs.com/pcwallet/"
			}
		],
		"dmg": {
			"title": "InWeCrypto",
			"icon": "./favicon.icns",
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			]
		},
		"win": {
			"target": "nsis",
			"icon": "./favicon.ico"
		}
	},
